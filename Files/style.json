    //editor
    "editor.tokenColorCustomizations": {
        "comments": "#ff5b5b",
        "textMateRules": [

//--------------------------- CLASS ------------------------------------------//
            { //class
                "scope": "storage.type.class.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //class name
                "scope": "entity.name.type.class.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //class parameter
                "scope": "support.type.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //class ()
                "scope": "meta.class.inheritance.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //class :
                "scope": "punctuation.section.class.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //imbetted class
                "scope": "entity.other.inherited-class.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },


//--------------------------- Function ------------------------------------------//          
            { //Function Call
                "scope": "meta.function-call.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //Function Build in
                "scope": "support.function.builtin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //decorator
                "scope": "entity.name.function.decorator.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //Constructor
                "scope": "support.function.magic.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //def
                "scope": "storage.type.function.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //function name
                "scope": "entity.name.function.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // ()
                "scope": "punctuation.definition.arguments.begin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // ()
                "scope": "punctuation.definition.arguments.end.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // ()
                "scope": "punctuation.definition.parameters.begin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // ()
                "scope": "punctuation.definition.parameters.end.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //parameter self
                "scope": "variable.parameter.function.language.special.self.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //parameter
                "scope": "variable.parameter.function.language.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // :
                "scope": "punctuation.section.class.begin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // lambda
                "scope": "storage.type.function.lambda.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // lambda ()
                "scope": "punctuation.parenthesis.end.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { // lambda ()
                "scope": "punctuation.parenthesis.begin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },


//--------------------------- Import ------------------------------------------//          
            { //import
                "scope": "keyword.control.import.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //from // if else
                "scope": "keyword.control.flow.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //import source
                "scope": "source.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },


//--------------------------- VAR ------------------------------------------//          
            { //string
                "scope": "string.python",
                "settings": {
                    "foreground": "#379252"
                } 
            },

            { //number
                "scope": "constant.numeric.dec.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //var
                "scope": "variable.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //var built in
                "scope": "variable.legacy.builtin.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //string in ''
                "scope": "string.quoted.single.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //const
                "scope": "constant.numeric.float.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //const built in
                "scope": "constant.language.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //self as var
                "scope": "variable.language.special.self.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },


//--------------------------- operators ------------------------------------------// 
            { //operator
                "scope": "keyword.operator.assignment.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //+
                "scope": "keyword.operator.arithmetic.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //==
                "scope": "keyword.operator.comparison.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //operator
                "scope": "keyword.operator.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

            { //operator word
                "scope": "source.python keyword.operator.logical.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },


//--------------------------- Seperator ------------------------------------------// 
            { //operator
                "scope": "punctuation.separator.parameters.python",
                "settings": {
                    "foreground": "#000000"
                } 
            },

        ]
    },

    "[json]": {



     
    },
    "workbench.colorTheme": "Atom One Dark"
}